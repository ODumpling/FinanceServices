@inject SignInManager<ApplicationUser> _signInManager
@{
    var hasExternalLogins = (await _signInManager.GetExternalAuthenticationSchemesAsync()).Any();
}
               
<a href="~/" class="text-cyan-100 hover:text-white hover:bg-cyan-600 group flex items-center px-2 py-2 text-sm leading-6 font-medium rounded-md" id="home">
    <!-- Heroicon name: outline/clock -->
    <svg class="mr-4 flex-shrink-0 h-6 w-6 text-cyan-200" xmlns="http://www.w3.org/2000/svg" fill="none" viewBox="0 0 24 24" stroke="currentColor" aria-hidden="true">
        <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M12 8v4l3 3m6-3a9 9 0 11-18 0 9 9 0 0118 0z"/>
    </svg>
    Home
</a>


